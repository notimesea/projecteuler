    matrix A(coefs.size(), vector <long long>(coefs.size()));

    for (int i = 0; i < coefs.size(); ++i) {
        A[0][i] = coefs[i];
        if (i) A[i][i - 1] = 1;
    }

    long long need = 100000000000000ll - 1;

    A = pw(A, need);

    long long ans = 0;

    for (int i = 0; i < coefs.size(); ++i) {
        ans += A[coefs.size() - 1][i] * initials[coefs.size() - 1 - i];
        ans %= mod;
    }

    cout << ans << "\n";


